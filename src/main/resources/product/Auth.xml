<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="product.dao.AuthDao">
    <resultMap id="userResultMap" type="product.model.Auth">
        <id property="userId" column="USER_ID" />
        <result property="phone" column="PHONE"/>
        <result property="regUserId" column="REG_USER_ID"/>
        <result property="regDtm" column="REG_DTM"/>
        <result property="modUserId" column="MOD_USER_ID"/>
        <result property="modDtm" column="MOD_DTM"/>
    </resultMap>

    <select id="selectAuth"  resultMap="userResultMap" >
      SELECT * FROM TB_USER_F

        /*product.dao.AuthDao.selectAuth*/
    </select>

</mapper>